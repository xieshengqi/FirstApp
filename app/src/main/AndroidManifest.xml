<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xieshengqi.myapplication">


    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name="com.example.xieshengqi.activitys.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name="com.example.xieshengqi.activitys.ListViewActivity" />
        <activity android:name="com.example.xieshengqi.activitys.DrawlayoutActivity" />
        <activity android:name="com.example.xieshengqi.activitys.SwipeRefreshActivity" />
        <activity android:name="com.example.xieshengqi.activitys.FlowLayoutActivity" />
        <activity android:name="com.example.xieshengqi.activitys.PullRefreshActivity" />
        <activity android:name="com.example.xieshengqi.activitys.MyPercentFrameLayoutActivity" />
        <activity android:name="com.example.xieshengqi.activitys.TabViewpagerActivity" />

        <meta-data android:name="design_width" android:value="768"></meta-data>
        <meta-data android:name="design_height" android:value="1280"></meta-data>
    </application>

</manifest>
